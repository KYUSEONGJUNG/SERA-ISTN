<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <!-- 프로퍼티 정보 설정 -->
    <bean name="propertiesService" class="org.egovframe.rte.fdl.property.impl.EgovPropertyServiceImpl" destroy-method="destroy">
        <property name="properties">
            <map>
                <entry key="pageUnit" value="10"/>
                <entry key="pageSize" value="10"/>
                <entry key="posblAtchFileSize" value="5242880"/>
                <entry key="Globals.fileStorePathSr" value="${Globals.fileStorePathSr}"/>
                <entry key="Globals.fileStorePath" value="${Globals.fileStorePath}"/>
                <entry key="Globals.addedOptions" value="false"/>
                <entry key="Globals.fileStorePathConnect" value="${Globals.fileStorePathConnect}"/>
                <entry key="Globals.DevelopFlag" value="${Globals.DevelopFlag}"/>
                <entry key="Globals.DevelopMailAddress" value="${Globals.DevelopMailAddress}"/>
                <entry key="addr.confirmKey" value="${addr.confirmKey}"/>
            </map>
        </property>
    </bean>
</beans>    
